{
	"model": {
		"type": "condition",
		"property": "has_component",
		"component": "custom_name",
		"on_true": {
			"type": "select",
			"model": "item/copper_shovel",
			"property": "component",
			"component": "custom_name",
	  		"cases": [
					{
				"when": [
						"Ladle",
						"Spoon"
					],
				"model": {
					"type": "model",
					"model": "item/copper_shovel/ladle"
			 			}
					},
					{
				"when": "Super Cute Shovel",
				"model": {
					"type": "model",
					"model": "item/copper_shovel/super_cute_shovel"
			 			}
					},
					{
				"when": "Kemetian Shovel",
				"model": {
					"type": "model",
					"model": "item/copper_shovel/kemetian_shovel"
			 			}
					},
					{
				"when": "Hellenic Shovel",
				"model": {
					"type": "model",
					"model": "item/copper_shovel/hellenic_shovel"
			 			}
					},
					{
				"when": "Sinitic Shovel",
				"model": {
					"type": "model",
					"model": "item/copper_shovel/sinitic_shovel"
			 			}
					},
					{
				"when": "Fantasy Shovel",
				"model": {
					"type": "model",
					"model": "item/copper_shovel/fantasy_shovel"
			 			}
					},
					{
				"when": "Norroen Shovel",
				"model": {
					"type": "model",
					"model": "item/copper_shovel/norroen_shovel"
			 			}
					},
					{
				"when": "Halloween Shovel",
				"model": {
					"type": "model",
					"model": "item/copper_shovel/halloween_shovel"
			 			}
					}
		  		],
			"fallback": {
				"type": "range_dispatch",
				"property": "damage",
				"scale": 4,
				"entries": [
					{
						"threshold": 0,
						"model": {
							"type": "model",
							"model": "item/copper_shovel"
						}
					},
					{
						"threshold": 1,
						"model": {
							"type": "model",
							"model": "item/copper_shovel/copper_shovel_damaged_0"
						}
					},
					{
						"threshold": 2,
						"model": {
							"type": "model",
							"model": "item/copper_shovel/copper_shovel_damaged_1"
						}
					},
					{
						"threshold": 3,
						"model": {
							"type": "model",
							"model": "item/copper_shovel/copper_shovel_damaged_2"
						}
					}
				]
			}
		},
		"on_false": {
			"type": "model",
						"model": "item/copper_shovel"
					}
		}
	}