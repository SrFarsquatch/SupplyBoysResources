{
  "model": {
    "type": "condition",
    "property": "has_component",
    "component": "item_name",
    "on_true": {
      "type": "select",
      "model": "item/piglin_spawn_egg",
      "property": "component",
      "component": "item_name",
      "cases": [
        {
          "when": [
            {
              "text": "Piglin Template",
              "italic": false
            }
          ],
          "model": {
            "type": "model",
            "model": "item/spawn_egg/piglin_template"
          }
        }
      ],
      "fallback": {
        "type": "model",
        "model": "item/piglin_spawn_egg"
      }
    },
    "on_false": {
      "type": "model",
      "model": "item/piglin_spawn_egg"
    }
  }
}